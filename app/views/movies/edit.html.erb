<%= form_for @movie do |f| %>
  <%= f.label :title %>
   <br />
  <%= f.text_field :title%>
  <br />
  <%= f.label :description %>
   <br />
  <%= f.text_area :description%>
  <br />
  <%= f.label :year_released %>
   <br />
  <%= f.text_field :year_released%>
  <br />
  <br />
  <%= f.submit "Submit Your Changes" %>
<% end %>
